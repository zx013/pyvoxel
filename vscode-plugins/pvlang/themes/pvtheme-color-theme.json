{
	"name": "pvtheme",
	"type": "light",
	"colors": {
		"editor.background": "#fffcf2",
		"editor.foreground": "#313b3f"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#b678e9"
			}
		},
		{
			"name": "Define Class",
			"scope": "defineclass.base",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Brackets Class",
			"scope": "defineclass.brackets",
			"settings": {
				"foreground": "#8346e6"
			}
		},
		{
			"name": "Inherit Class",
			"scope": "defineclass.inherit",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9370DB"
			}
		},
		{
			"name": "Inherit Alias Class",
			"scope": "defineclass.inheritalias",
			"settings": {
				"foreground": "#9370DB"
			}
		},
		{
			"name": "Alias Class",
			"scope": "defineclass.alias",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Arrow Class",
			"scope": "defineclass.arrow",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Find Class",
			"scope": "findclass.base",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Brackets Class",
			"scope": "findclass.brackets",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Alias Class",
			"scope": "findclass.alias",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Allow Class",
			"scope": "findclass.arrow",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Attribute Variable",
			"scope": "attribute.variable",
			"settings": {
				"foreground": "#db781c"
			}
		},
		{
			"name": "Attribute Brackets",
			"scope": "attribute.brackets",
			"settings": {
				"foreground": "#dbae1c"
			}
		},
		{
			"name": "Attribute Note Keywords",
			"scope": "attribute.note.keywords",
			"settings": {
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "Attribute Expr Variable",
			"scope": "attribute.expr.variable",
			"settings": {
				"foreground": "#0f1888"
			}
		}
	]
}